        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Canvas Background="Black">
        <Image x:Name="image" Height="152" Canvas.Left="334" Canvas.Top="10" Width="173" Source="Images/SnoopDog.jpg"/>
        <Image x:Name="image1" Height="124" Canvas.Left="27" Canvas.Top="125" Width="125" Source="Images/MazeLogo.jpg"/>
        <Button x:Name="SinglePlayer" Content="Single Player" Canvas.Left="200" Canvas.Top="106" Width="120" Height="27" Click="SinglePlayer_Click"/>
        <Button x:Name="MultiPlayer" Content="MultiPlayer" Canvas.Left="200" Canvas.Top="157" Width="120" Height="26" Click="MultiPlayer_Click"/>
        <Button x:Name="Settings" Content="Settings" Canvas.Left="200" Canvas.Top="208" Width="120" Height="24" Click="Settings_Click"/>


    </Canvas>

</Window>
